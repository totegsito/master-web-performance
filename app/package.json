{
  "description": "Start package for Bulma",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@parcel/compressor-brotli": "^2.5.0",
    "@parcel/compressor-gzip": "^2.5.0",
    "@parcel/config-default": "^2.5.0",
    "@parcel/transformer-sass": "^2.5.0",
    "autoprefixer": "^9.7.6",
    "babel-plugin-import": "^1.13.5",
    "buffer": "^6.0.3",
    "bulma": "^0.8.2",
    "cross-env": "^7.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "process": "^0.11.10"
  },
  "keywords": [
    "sass",
    "bulma",
    "css",
    "start"
  ],
  "license": "MIT",
  "name": "bulma-start",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgthms/bulma-start.git"
  },
  "scripts": {
    "css-build": "node-sass _sass/main.scss css/main.css",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-postcss": "postcss --use autoprefixer --output css/main.css css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "js-build": "parcel src/*",
    "js-watch": "npm run js-build -- --watch",
    "start": "cross-env npm-run-all --parallel css-watch js-watch"
  },
  "version": "0.0.4",
  "dependencies": {
    "axios": "^0.27.2"
  }
}
